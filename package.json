{
  "name": "cra-rr4-redux-antd",
  "title": "cra rr4 redux antd",
  "description": "Create React App, React Router v4, Rematch and Ant Design",
  "keywords": [
    "template",
    "react",
    "rematch",
    "router",
    "antd",
    "app",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelcast/cra-rr4-redux-antd.git"
  },
  "bugs": {
    "url": "https://github.com/miguelcast/cra-rr4-redux-antd/issues"
  },
  "version": "0.3.0",
  "private": false,
  "license": "MIT",
  "author": "Miguel Cast",
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "dependencies": {
    "@rematch/core": "^1.0.6",
    "@rematch/loading": "^1.1.2",
    "@rematch/persist": "^1.1.5",
    "antd": "^3.10.3",
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5"
  },
  "css": {
    "preprocess": "less"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --trailing-comma all --jsx-bracket-same-line --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive --scripts-prepend-node-path",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react-hooks": "^1.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "source-map-explorer": "^1.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
